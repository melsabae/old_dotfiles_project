URxvt.scrollBar: false
URxvt.urgentOnBell: true

URxvt.depth: 32

URxvt.perl-lib: /usr/lib/urxvt/perl
URxvt.perl-ext-common: default,matcher,vtwheel,clipboard
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-].[\\w/-]
URxvt.matcher.button: 3
URxvt.url-launcher: /usr/bin/qutebrowser


URxvt.font: xft:Hack-Regular:size=13, \
						xft:Envy Code R:size=17:style=Regular, \
						xft:DejaVuSansMono:pixelsize=17:style=Regular
URxvt.letterSpace: 0
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgba
Xft.lcdfilter: lcddefault
Xft.autohint: true


